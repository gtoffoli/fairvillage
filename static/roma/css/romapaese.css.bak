	.segna{
		position: relative;
		display: inline-block;
		background: #A63840; /*#ff4200;*/
		color: #fff;
		outline: 0;
		transition: background-color .25s ease-in-out;
		-moz-transition: background-color .25s ease-in-out;
		-webkit-transition: background-color .25s ease-in-out;
		height: 22px;
		line-height:20px;
		padding: 2px 16px 0 16px;
		/*font-variant:small-caps;*/
		font-weight:bold;
		font-size: 14px;
		text-transform:uppercase;
	}
.segna:after
		{
			content: '';
			display: block;
			position: absolute;
			right: -12px;
			top: 0;
			border-left: solid 12px #A63840; /*#ff4200;*/
			border-top: solid 12px transparent;
			border-bottom: solid 12px transparent;
			transition: border-left-color .25s ease-in-out;
			-moz-transition: border-left-color .25s ease-in-out;
			-webkit-transition: border-left-color .25s ease-in-out;
		}
	.segna-small{
		position: relative;
		display: inline-block;
		background: #A63840; /*#ff4200;*/
		color: #fff;
		outline: 0;
		transition: background-color .25s ease-in-out;
		-moz-transition: background-color .25s ease-in-out;
		-webkit-transition: background-color .25s ease-in-out;
		height: 18px;
		line-height:18px;
		padding: 0 9px 0 9px;
		/*font-variant:small-caps;*/
		font-weight:bold;
		font-size: 12px;
		text-transform:uppercase;
	}

.segna-small:after
		{
			content: '';
			display: block;
			position: absolute;
			right: -6px;
			top: 0;
			border-left: solid 6px #A63840; /*#ff4200;*/
			border-top: solid 9px transparent;
			border-bottom: solid 9px transparent;
			transition: border-left-color .25s ease-in-out;
			-moz-transition: border-left-color .25s ease-in-out;
			-webkit-transition: border-left-color .25s ease-in-out;
		}


