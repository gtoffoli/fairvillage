/* bootstrap.css p.2876 */
.dropdown {
	z-index:10000;
	}
/* bootstrap.css p.2905 */	
.dropdown-menu {
  border: 1px solid #047FAD;
  border: 1px solid rgba(4, 127, 173, 0.4);
  -webkit-box-shadow: 0 5px 10px rgba(4, 127, 173, 0.4);
     -moz-box-shadow: 0 5px 10px rgba(4, 127, 173, 0.4);
          box-shadow: 0 5px 10px rgba(4, 127, 173, 0.4);
}
/* bootstrap.css p.2957 */
.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus,
.dropdown-submenu:hover > a,
.dropdown-submenu:focus > a {
  background-color: #38AFDC;
  background-image: -moz-linear-gradient(top, #9DE7FF, #38AFDC);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#9DE7FF), to(#38AFDC));
  background-image: -webkit-linear-gradient(top, #9DE7FF, #38AFDC);
  background-image: -o-linear-gradient(top, #9DE7FF, #38AFDC);
  background-image: linear-gradient(to bottom, #9DE7FF, #38AFDC);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#9DE7FF', endColorstr='#38AFDC', GradientType=0);
}
/* bootstrap.css p.2972 */
.dropdown-menu > .active > a,
.dropdown-menu > .active > a:hover,
.dropdown-menu > .active > a:focus {
  background-color: #38AFDC;
  background-image: -moz-linear-gradient(top, #9DE7FF, #38AFDC);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#9DE7FF), to(#38AFDC));
  background-image: -webkit-linear-gradient(top, #9DE7FF, #38AFDC);
  background-image: -o-linear-gradient(top, #9DE7FF, #38AFDC);
  background-image: linear-gradient(to bottom, #9DE7FF,#38AFDC);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#9DE7FF', endColorstr='#38AFDC', GradientType=0);
}
